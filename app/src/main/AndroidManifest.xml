<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.sergeenko.alexey.noble">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".NobleApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:requestLegacyExternalStorage="true"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Noble"
        android:usesCleartextTraffic="true">
        <activity android:name=".ClientEditActivity"></activity>
        <activity android:name=".NewClientActivity" />
        <activity android:name=".StartActivity" />
        <activity android:name=".AgreementActivity" />
        <activity
            android:name=".PasswordRecoveryActivity"
            android:label="@string/title_activity_password_recovery" />
        <activity android:name=".AuthActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>